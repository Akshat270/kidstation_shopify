<script>
  // Copy to clipboard
  document.addEventListener('DOMContentLoaded', function() {
    const copyButtons = document.querySelectorAll('.sp_copy_btn');
    copyButtons.forEach(button => {
      button.addEventListener('click', function (event) {
        event.stopPropagation();
        const code = this.getAttribute('data-code');
        navigator.clipboard.writeText(code)
          .then(() => {
            this.classList.add('sp_copied');
            this.textContent = "Copied!";
            setTimeout(() => {
              this.classList.remove('sp_copied');
              this.textContent = "Copy";
            }, 1500);
          })
          .catch(err => {
            console.error('Copy failed: ', err);
          });
      });
    });

    // Dragging functionality
    const container = document.getElementById('sp_coupon_scroll');
    let isDown = false;
    let startX;
    let scrollLeft;

    container.addEventListener('mousedown', (e) => {
      isDown = true;
      container.classList.add('active');
      startX = e.pageX - container.offsetLeft;
      scrollLeft = container.scrollLeft;
    });

    container.addEventListener('mouseleave', () => {
      isDown = false;
    });

    container.addEventListener('mouseup', () => {
      isDown = false;
    });

    container.addEventListener('mousemove', (e) => {
      if (!isDown) return;
      e.preventDefault();
      const x = e.pageX - container.offsetLeft;
      const walk = (x - startX) * 1; // Adjust speed
      container.scrollLeft = scrollLeft - walk;
    });
  });
</script>

