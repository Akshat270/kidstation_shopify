  .custom-slider-wrapper {
    overflow-x: auto;
    scroll-snap-type: x proximity;
    scroll-behavior: smooth;
    -webkit-overflow-scrolling: touch;
    padding-bottom: 10px;
    cursor: grab;
    user-select: none;
  }

  .custom-slider-track {
    display: flex;
    gap: 16px;
    padding: 0 10px;
  }

  .custom-slider-slide {
    flex: 0 0 66.6%;
    scroll-snap-align: start;
    border-radius: 12px;
    overflow: hidden;
    box-shadow: 0 4px 10px rgba(0,0,0,0.1);
    transition: transform 0.3s, box-shadow 0.3s;
  }
  .custom-slider-slide:hover {
    transform: translateY(-5px);
    box-shadow: 0 6px 15px rgba(0,0,0,0.15);
  }
  .custom-slider-slide img {
    width: 100%;
    display: block;
    object-fit: cover;
    pointer-events: none;
  }

  .custom-slider-wrapper::-webkit-scrollbar {
    height: 6px;
  }
  .custom-slider-wrapper::-webkit-scrollbar-thumb {
    background: #ddd;
    border-radius: 6px;
  }
  .custom-slider-wrapper::-webkit-scrollbar-thumb:hover {
    background: #ccc;
  }

  @media (min-width: 768px) {
    .custom-slider-slide {
      flex: 0 0 45%;
    }
  }
</style>

<script>
  document.addEventListener('DOMContentLoaded', () => {
    const slider = document.getElementById('customSlider');
    let isDown = false, startX, baseScroll, targetScroll, raf;

    slider.querySelectorAll('img').forEach(img => img.draggable = false);

    function smoothScroll() {
      slider.scrollLeft += (targetScroll - slider.scrollLeft) * 0.1;
      if (isDown) raf = requestAnimationFrame(smoothScroll);
    }

    slider.addEventListener('mousedown', e => {
      isDown = true;
      slider.classList.add('dragging');
      startX = e.pageX - slider.offsetLeft;
      baseScroll = slider.scrollLeft;
      targetScroll = baseScroll;
      cancelAnimationFrame(raf);
      e.preventDefault();
    });

    slider.addEventListener('mousemove', e => {
      if (!isDown) return;
      const x = e.pageX - slider.offsetLeft;
      targetScroll = baseScroll - (x - startX) * 1.5;
      cancelAnimationFrame(raf);
      raf = requestAnimationFrame(smoothScroll);
    });

    ['mouseup', 'mouseleave'].forEach(evt =>
      slider.addEventListener(evt, () => {
        if (!isDown) return;
        isDown = false;
        slider.classList.remove('dragging');
        cancelAnimationFrame(raf);
      })
    );

    slider.addEventListener('touchstart', e => {
      isDown = true;
      startX = e.touches[0].pageX - slider.offsetLeft;
      baseScroll = slider.scrollLeft;
    }, { passive: true });

    slider.addEventListener('touchmove', e => {
      if (!isDown) return;
      const x = e.touches[0].pageX - slider.offsetLeft;
      slider.scrollLeft = baseScroll - (x - startX) * 1.5;
    }, { passive: true });

    slider.addEventListener('touchend', () => isDown = false, { passive: true });
  });
</script>
